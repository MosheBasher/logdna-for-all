{
  "name": "logz-for-all",
  "version": 2,
  "builds": [{
      "src": "zeitConnector/integrationConfigServer.js",
      "use": "@now/node"
    },
    {
      "src": "zeitConnector/redirectServer.js",
      "use": "@now/node"
    }
  ],
  "routes": [{
      "src": "/ui-hooks",
      "dest": "zeitConnector/integrationConfigServer.js"
    },
    {
      "src": "/redirect",
      "dest": "zeitConnector/redirectServer.js"
    }
  ],
  "github": {
    "silent": true
  },
  "env": {
    "VERSION": "1.3.1",
    "INTEGRATION_CLIENT_ID": "@integration_client_id",
    "INTEGRATION_CLIENT_SECRET": "@integration_client_secret"
  }
}